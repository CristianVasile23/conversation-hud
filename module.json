{
  "id": "conversation-hud",
  "title": "ConversationHUD",
  "description": "<p>A simple module that adds a nice HUD that display the portraits of all the characters present in a conversation.</p>",
  "version": "#{VERSION}#",
  "compatibility": {
    "minimum": "13",
    "verified": "13",
    "maximum": "13"
  },
  "authors": [
    {
      "name": "Saervicus (Cristian Vasile)",
      "discord": "Saervicus#7305",
      "url": "https://github.com/CristianVasile23",
      "flags": {}
    }
  ],
  "relationships": {
    "requires": [
      {
        "id": "lib-wrapper",
        "type": "module",
        "compatibility": {
          "minimum": "1.12.4.0",
          "verified": "1.12.13.1"
        }
      },
      {
        "id": "socketlib",
        "type": "module",
        "compatibility": {
          "minimum": "1.0.10",
          "verified": "1.0.13"
        }
      },
      {
        "id": "lib-dfreds-ui-extender",
        "type": "module",
        "compatibility": {}
      }
    ]
  },
  "esmodules": ["js/init.js"],
  "styles": [
    {
      "src": "css/main.css"
    },
    {
      "src": "font/font.css"
    }
  ],
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "lang/en.json",
      "flags": {}
    },
    {
      "lang": "fr",
      "name": "Fran√ßais",
      "path": "lang/fr.json",
      "flags": {}
    }
  ],
  "socket": true,
  "url": "#{URL}#",
  "manifest": "#{MANIFEST}#",
  "download": "#{DOWNLOAD}#",
  "license": "https://github.com/CristianVasile23/conversation-hud/blob/main/LICENSE",
  "readme": "https://github.com/CristianVasile23/conversation-hud/blob/main/README.md",
  "media": [
    {
      "type": "setup",
      "url": "https://raw.githubusercontent.com/CristianVasile23/conversation-hud/main/docs/icons/setup.jpg",
      "loop": false,
      "flags": {}
    },
    {
      "type": "cover",
      "url": "https://raw.githubusercontent.com/CristianVasile23/conversation-hud/main/docs/icons/cover.jpg",
      "loop": false,
      "flags": {}
    }
  ],
  "flags": {}
